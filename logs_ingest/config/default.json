{
  "name": "default",
  "displayName": "Default",
  "rules": [
    {
      "sources": [],
      "attributes": [
        {
          "key": "cloud.region",
          "pattern": "location"
        },
        {
          "key": "timestamp",
          "pattern": "time"
        },
        {
          "key": "log.source",
          "pattern": "if(properties.eventCategory == null, &category, &(join('', ['Activity Log - ', category])), @)"
        },
        {
          "key": "content",
          "pattern": "properties"
        }
      ]
    }
  ]
}
